<html>
  <head>
    <title>test.json 데이터 보기 ejs.ver</title>
    <!-- Minified version -->
    <link rel="stylesheet" href="https://cdn.simplecss.org/simple.min.css" />

    <!-- Un-Minified version -->
    <link rel="stylesheet" href="https://cdn.simplecss.org/simple.css" />
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>제목</th>
          <th>내용</th>
        </tr>
      </thead>
      <tbody>
        <% posts.forEach((post)=>{ %>
        <tr>
          <td><%= post['title']%></td>
          <td><%= post['content']%></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
